<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript" src="~/Scripts/knockout-3.4.2.js"></script>
    <script type="text/javascript" src="~/Scripts/underscore.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/DashboardModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/RoleModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/TeamModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/UserModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/BankHolidayModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/MonthDayModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/VacationModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/CalendarModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Dashboard/VacationStateModel.js"></script>

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Holiday Managament DVSE", "Index", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - HolidayManagement DVSE</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
