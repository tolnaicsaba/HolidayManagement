<h1><strong>Calendar</strong></h1>
<div>
    <p><strong>
    <span data-bind="text: month"></span>
    <span data-bind="text: year"></span>
</strong></p>
    <button type="button" class="btn btn-info btn-md">Previous Month</button>
    @Html.Partial("_AddHoliday")
    <button type="button" class="btn btn-info btn-md pull-right">Next Month</button>
    <br /><br />
    <table class="table table-hover">
            <tbody data-bind="foreach: calendar">
                <tr>
                    <td>
                        <div class="day">
                            <span data-bind="text: day"></span>
                            <span data-bind="text: name"></span>

                        </div>
                    </td>
                </tr>
            </tbody>
    </table>
    </div>